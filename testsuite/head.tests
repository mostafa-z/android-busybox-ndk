. ./testing.sh

# testing "test name" "command" "expected result" "file input" "stdin"

cat <<EOF > head.input
line 1
line 2
line 3
line 4
line 5
line 6
line 7
line 8
line 9
line 10
line 11
line 12
EOF

testing "head -n <negative number>" \
	"head -n -9 head.input" \
	"line 1\nline 2\nline 3\n" \
	"" ""

rm head.input

exit $FAILCOUNT
